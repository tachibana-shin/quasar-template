{
  "name": "quasar-app",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "pretit": "prettier \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "pnpm test:unit:ci",
    "test:watch": "pnpm test:unit",
    "dev": "quasar dev",
    "typing": "vue-tsc --noEmit",
    "build": "quasar build",
    "ci": "pnpm format && pnpm lint --fix && pnpm typing && pnpm test:unit:ci",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.1",
    "@quasar/extras": "^1.15.10",
    "@vueuse/core": "^9.12.0",
    "@vueuse/head": "^1.0.23",
    "@vueuse/shared": "^9.12.0",
    "dayjs": "^1.11.7",
    "iso-639-1": "^2.1.15",
    "onu-ui": "1.0.14-beta.2",
    "pinia": "^2.0.29",
    "pinia-plugin-persistedstate": "^3.0.2",
    "quasar": "^2.12.2",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-request": "2.0.0-rc.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@hannoeru/eslint-config": "^0.9.5",
    "@hannoeru/eslint-plugin": "^0.9.5",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
