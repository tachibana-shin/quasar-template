    "@quasar/cli": "^2.0.0",
    "@quasar/icongenie": "^2.5.4",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.3.0",
    "@quasar/vite-plugin": "^1.4.1",
    "@tachibana-shin/eslint-config": "^1.0.7",
    "@tachibana-shin/vite-plugin-pages": "0.31.1-beta.0",
    "@testing-library/vue": "^7.0.0",
    "@types/node": "18.8.0",
    "@types/serviceworker": "^0.0.67",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@unocss/transformer-directives": "^0.53.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.31.0",
    "@vitest/web-worker": "^0.30.1",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "bumpp": "^8.2.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.9.0",
    "fake-indexeddb": "^4.0.1",
    "happy-dom": "^9.20.3",
    "jsdom": "^21.1.0",
    "kleur": "^4.1.5",
    "prettier": "^2.8.3",
    "prompts": "^2.4.2",
    "sass": "1.32.11",
    "tsx": "^3.12.2",
    "typescript": "^5.0.4",
    "unocss": "^0.53.4",
